<!-- app/views/users/preference_access.html.erb -->
<!DOCTYPE html>
<html>
<%= link_to 'Open Workshift Preference Form for Everyone', preference_access_path(:change_preference_for_id => "all"), :class=>"btn btn-theme" %>
<%= link_to 'Close Workshift Preference Form for Everyone', preference_access_path(:change_preference_for_id => "none"), :class=>"btn btn-theme" %>
<div class="col-md-5">
  <table id="Workshifts_uploaded" class="table table-striped table-hover">
    <tr>
      <th>Name</th>
    </tr>
    <% @users.each do | user | %>
      <tr>
        <td>
          <%=user.full_name %>
        </td>
      </tr>
    <% end %>
  </table>
</div>

</html>