<%=form_tag admin_edit_user_info_path do %>
  <%= render "edit_permissions_form" %>
  <div class="form-group">
        <div class="col-sm-2 control-label">
            <label for="compensated_hours">Compensated Hours</label>
        </div>
        <div class="col-sm-10" style='margin-bottom:25px;'>
            <div class="input-group">
              <input id="compensated_hours" type="text" name="user[compensated_hours]" style="max-width:500px;"class="form-control">
            </div>
        </div>
    </div>
  <button type="submit" class="btn btn-theme" id="submit_button">Update User</button>
<% end %>